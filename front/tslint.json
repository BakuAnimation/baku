{
  "rulesDirectory": [
    "codelyzer",
    "tslint-consistent-codestyle"
  ],
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "rxjs-tslint-rules"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "indent": [
      true,
      "spaces"
    ],
    "no-console": false,
    "no-empty": false,
    "interface-name": false,
    "space-before-function-parent": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ]
  }
}